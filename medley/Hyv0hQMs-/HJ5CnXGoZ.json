{"_id":"HJ5CnXGoZ","title":"Regex","body":{"blocks":[{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"ag6qs","text":"Regex","type":"header-one"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"9717s","text":"Preg_match(\"a\", \"bcit\") //false","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"7nodt","text":"(\"/a/i\", \"bcit\"); // '/' used as start and finish, 'i' == case insenstive","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"5qqr4","text":"(\"#a#i\", \"bcit\"); // same as above, less used","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"fimc5","text":"(\"/http:\\/\\//\" \"bcit\"); // '\\' == reminds no the end","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"479i3","text":"(\"/a|b/\") //a or b","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"bk012","text":"(\"/(ab)|(cd)/\"); // either ab or cd","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"bbbcj","text":"(\"/a*/\"); // 0 or more 'a'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"37qjm","text":"(\"/ab*c/\"); //ac, aac, abc, !abxc it says 'a' and either 0 or more 'b' in the middle then follow by 'c'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"f4j9m","text":"(\"/a+/\"); //1 or more 'a'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"f677t","text":"(\"/5(xy+(abc*d)*)+6/\"); //hortest match: 5xy6  !5xyabc6 because \"abc*d\" is in () it means we have either all or none","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"3q43d","text":"(\"/a?/\"); //0 or 1","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"4peu2","text":"(\"/ab?c/\"); //ac abc !cbc !abbc","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"fi936","text":"(\"/./\"); //any single char","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"1dv0j","text":"(\"/a{3}/\"); //at lease three 'a'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"1kbdf","text":"(\"/a{2,4}/\"); //2 to 4 'a'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"1ohs5","text":"(\"/a{2,}/\"); //at least 2 'a'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"afaut","text":"(\"/a{,4}/\"); //up  to 4 'a'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"77j3e","text":"(\"/^a/\"); //must begin with a","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"ed04g","text":"('/z$/'); //must end with z becarefull when use '$' always use single ' so its no var","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"3kl6r","text":"(\"/[aeiou]/\"); //any single chars in the list","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"3imaf","text":"(\"/[a-z]/\"); //range from 'a' to 'z'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"7rig","text":"(\"/[a-zA-Z0-9]/\"); //","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"b9b34","text":"(\"/[0-9-]/\"); // 0 to 9 or '-'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"pokh","text":"(\"/[^aeiou]/\"); //not any char from the list","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"f2p97","text":"(\"/[a^eiou]/\"); //any of those 6 chars","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"a2nek","text":"(\"/^[^^]/\"); //start with no '^'","type":"custom-code-block-cpp"},{"data":{},"depth":0,"entityRanges":[],"inlineStyleRanges":[],"key":"d6b7s","text":"('/[01][0-9](:[0-5][0-9]){2}$/'); //hh:mm:ss","type":"custom-code-block-cpp"}],"entityMap":{}},"dateCreated":"2017-09-22T06:25:22.466Z","lastUpdated":"2017-10-11T00:48:34.209Z","notebookId":"Hyv0hQMs-"}
